			},
			"ap-south-1": {
				"QualixAMI": "ami-3c3a4f53",
				"ESAMI": "ami-2b265344",
				"NatAMI": "ami-e2b9d38d"
			},
			"ap-northeast-2": {
				"QualixAMI": "ami-7d11db13",
				"ESAMI": "ami-6017dd0e",
				"NatAMI": "ami-d14388bf"
			},
			"ap-southeast-1": {
				"QualixAMI": "ami-6b64bb08",
				"ESAMI": "ami-3c65ba5f",
				"NatAMI": "ami-a79b49c4"
			},
			"ap-southeast-2": {
				"QualixAMI": "ami-8ceedbef",
				"ESAMI": "ami-cdb085ae",
				"NatAMI": "ami-53371f30"
			},
			"ap-northeast-1": {
				"QualixAMI": "ami-73448012",
				"ESAMI": "ami-e43bff85",
				"NatAMI": "ami-2443b745"
			},
			"eu-central-1": {
				"QualixAMI": "ami-6cdd2a03",
				"ESAMI": "ami-69dd2a06",
				"NatAMI": "ami-5825cd37"
			},
			"sa-east-1": {
				"QualixAMI": "ami-1fa33373",
				"ESAMI": "ami-1ba23277",
				"NatAMI": "ami-9336bcff"
			}
		"AZFromRegion": {
			"us-east-1": {
				"AZ": "us-east-1a"
			},
			"us-west-2": {
				"AZ": "us-west-2a"
			},
			"us-west-1": {
				"AZ": "us-west-1b"
			},
			"eu-west-1": {
				"AZ": "eu-west-1c"
			},
			"eu-central-1": {
				"AZ": "eu-central-1b"
			},
			"ap-southeast-1": {
				"AZ": "ap-southeast-1a"
			},
			"ap-northeast-1": {
				"AZ": "ap-northeast-1a"
			},
			"ap-southeast-2": {
				"AZ": "ap-southeast-2a"
			},
			"ap-northeast-2": {
				"AZ": "ap-northeast-2a"
			},
			"sa-east-1": {
				"AZ": "sa-east-1a"
			},
			"ap-south-1": {
				"AZ": "ap-south-1a"
			}
		}
	},
	"Resources": {
		"VPCNAT": {
			"Type": "AWS::CloudFormation::Stack",
			"Properties": {
				"TemplateURL": "https://s3.amazonaws.com/cloudshell-cf/1_VPC_EX.json",
				"Parameters": {
					"VPCCIDR": {
						"Ref": "1VPCCIDR"
					},
					"ESCIDR": {
						"Ref": "2ESCIDR"
					},
					"VPNAddress": {
						"Ref": "5VPNAddress"
					},
					"OnPremiseCIDR": {
						"Ref": "4OnPremiseCIDR"
					},
					"ServerIP": {
						"Ref": "ZServerIP"
					},
					"KeyPair": {
						"Ref": "7KeyPair"
					},
					"QualiCIDR": {
						"Ref": "3QualiCIDR"
					},
					"AZ": {
						"Fn::FindInMap": ["AZFromRegion",
						{
							"Ref": "AWS::Region"
						},
						"AZ"]
					}
				}
			}
		},
		"Instances": {
			"Type": "AWS::CloudFormation::Stack",
			"Properties": {
				"Parameters": {
					"QualixSN": {
						"Fn::GetAtt": ["VPCNAT",
						"Outputs.QualixSN"]
					},
					"ESInstanceSN": {
						"Fn::GetAtt": ["VPCNAT",
						"Outputs.ESInstanceSN"]
					},
					"QualiXInstanceProfile": {
						"Fn::GetAtt": ["VPCNAT",
						"Outputs.QualiXInstanceProfile"]
					},
					"ESInstanceProfile": {
						"Fn::GetAtt": ["VPCNAT",
						"Outputs.ESInstanceProfile"]
					},
					"SG1id": {
						"Fn::GetAtt": ["VPCNAT",
						"Outputs.SG1id"]
					},
					"SG2id": {
						"Fn::GetAtt": ["VPCNAT",
						"Outputs.SG2id"]
					},
					"QualixAMI": {
						"Fn::FindInMap": ["AMIFromRegion",
						{
							"Ref": "AWS::Region"
						},
						"QualixAMI"]
					},
					"ESAMI": {
						"Fn::FindInMap": ["AMIFromRegion",
						{
							"Ref": "AWS::Region"
						},
						"ESAMI"]
					},
					"KeyPair": {
						"Ref": "7KeyPair"
					},
					"ESInstance2Profile": {
						"Fn::GetAtt": ["VPCNAT",
						"Outputs.ESInstance2Profile"]
					},
					"ServerIP": {
						"Ref": "ZServerIP"
					},
					"ZUsername": {
						"Ref": "ZUsername"
					},
					"ZPassword": {
						"Ref": "ZPassword"
					}
				},
				"TemplateURL": "https://s3.amazonaws.com/cloudshell-cf/2_EC2_EX.json"
			}
		}
	},
	"Outputs": {
		"ManagementVPCID": {
			"Description": "Management VPC ID",
			"Value": {
				"Fn::GetAtt": ["VPCNAT",
				"Outputs.VPCId"]
			}
		},
		"SG1id": {
			"Description": "SG1 ID",
			"Value": {
				"Fn::GetAtt": ["VPCNAT",
				"Outputs.SG1id"]
			}
		},
		"S3Name": {
			"Description": "Name of S3 bucket",
			"Value": {
				"Fn::GetAtt": ["VPCNAT",
				"Outputs.S3Name"]
			}
		},
		"QualixInstanceEIP": {
			"Description": "Qualix Instance Elastic IP",
			"Value": {
				"Fn::GetAtt": ["Instances",
				"Outputs.QualixInstanceEIP"]
			}
		}
	}
}